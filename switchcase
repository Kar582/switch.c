#include<stdio.h>
int main()
{
    int choice=0;
    printf("choose\n1.PIZZA\n2.BURGER\n3.PASTA\n4.FRENCH FRIES\n5.SANDWISH\n");
    scanf("%d",&choice);
    switch(choice)
    {
        case 1: printf("FOOD ITEM-PIZZA\nPRICE -RS 179");
                break;
        case 2: printf("FOOD ITEM-BURGER\nPRICE -RS 129");
                break;
        case 3: printf("FOOD ITEM-PASTA\nPRICE -RS 179");       
                break;
        case 4: printf("FOOD ITEM-FRENCH FRIES\nPRICE -RS 99"); 
                break;
        case 5: printf("FOOD ITEM-SANDWISH\nPRICE -RS 149"); 
                break;
        default: printf("NOT AVAILABLE"); 
        
    }
}
